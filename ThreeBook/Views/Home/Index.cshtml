@model IEnumerable<ThreeBook.Models.TypesBook>

@{
    ViewBag.Title = "Список категорий нашей библиотеки";
}

<h2>Список категорий нашей библиотеки</h2>
<img src="~/Content/book.jpg" />

<p>
    @Html.ActionLink("Создать новую категорию", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.title)
        </th>
        <th>@ViewBag.DateTime</th>
    </tr>

    @foreach (ThreeBook.Models.TypesBook item in Model)
    {
        <tr>
            <td>

                @Html.ActionLink(@Html.DisplayFor(modelItem => item.title).ToString(), "ListBooks", new { id = item.Id }) |
            </td>
            <td>
                @Html.ActionLink("Редактирование", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Посмотреть", "Details", new { id = item.Id }) |
                @Html.ActionLink("Удалить", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
<button onclick="GetFirstBook();">Get first Book</button>
<button onclick="GetLastBook();">Get last Book</button>
<button onclick="fetchMoviesJSON();">Get middle Book</button>

<script src="~/Scripts/IndexBible.js"></script>